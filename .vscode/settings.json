// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/__pycache__/**": true,
        ".vagrant/**": true,
        "data/**": true
    },
    "python.linting.flake8Enabled": true,
    "python.pythonPath": "${workspaceRoot}/venv/bin/python3.5"
}
